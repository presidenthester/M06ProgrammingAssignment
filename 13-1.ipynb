    "\n",
    "date_str = datetime.today().strftime('%Y-%m-%d')\n",
    "with open('today_string.txt', 'w') as f:\n",
    "    f.write(date_str)\n",
    "    \n",
    "with open('today_string.txt', 'r') as f:\n",
    "    today_string = f.read()\n",
    "    \n",
    "date_obj = datetime.strptime(today_string, '%Y-%m-%d')"
   ]
  }
